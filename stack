#include <stdio.h>
#include <stdlib.h>
#include <stdbool.h>

#define true 1
#define false 0
typedef struct stack
{
	int capacity;
	int top;
	int *arr;
}stack;

stack *
createStack (int capacity)
{
	stack *s = (stack *) malloc (sizeof(stack()));
	
	if (s == NULL) return NULL;
	s->top = -1; s->capacity = capacity;
	s->arr = (int *) malloc (sizeof (int) * capacity);
	return s;
}

bool
isFull(stack *s)
{
	if (s->capacity - 1 == s->top)
	{
		return true;
	}
	else
	{
		return false;
	}
}
void
push (stack *s, int data)
{
	if (isFull(s)) return;
	s->arr[++s->top] = data;
	printf ("\nPush to stack: %d", data);
}

bool
isEmpty(stack *s)
{
	if (s->top == -1) return true;
	else
	  return false;
}
int
pop (stack *s)
{
	if (isEmpty(s)) return -1;
	return s->arr[s->top--];
	
}
int main()
{
	stack *s =  createStack(100);
	
	push(s, 10); push(s, 20); push(s, 30);
	printf ("\nPop ele: %d", pop(s));
}
