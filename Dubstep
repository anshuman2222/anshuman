#include <iostream>
using namespace std;

void
dubset (char *ch)
{
    int flag = 0, tmp = 0;

    while (*ch)
    {
        if (ch[0] == 'W' && ch[1] == 'U' && ch[2] == 'B')
        {
            ch = ch + 3;
            flag++;
            continue;
        }
        if (tmp == 0 && flag)
        {
            tmp = 1;
            continue;
        }
        if (flag)
        {
            cout<<" ";
            flag = 0;
        }
        cout<<*ch;
        ch = ch + 1;

    }
}

int main()
{
    char ch[201] = {'\0'};
    cin>>ch;

    dubset (ch);
    return 0;
 }
