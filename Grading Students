#include <iostream>
#include <vector>

using namespace std;

vector < int > solve(vector < int > grades){
    int range;
    
    for (int grades_i = 0; grades_i < grades.size(); grades_i++)
    {
    	if (grades[grades_i] > 37)
    	{
    		range = grades[grades_i] + 5 - (grades[grades_i] % 5);
			if ((range - grades[grades_i]) < 3)
   		    {
   		    	grades[grades_i] = range;	
			}
		}
	}
	return grades;
}

int main() {
    int n;
    cin >> n;
    vector<int> grades(n);
    for(int grades_i = 0; grades_i < n; grades_i++){
       cin >> grades[grades_i];
    }
    vector < int > result = solve(grades);
    for (ssize_t i = 0; i < result.size(); i++) {
        cout << result[i] << (i != result.size() - 1 ? "\n" : "");
    }
    cout << endl;
    

    return 0;
}
